{
  "name": "azure-tenant-ai-chat",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "update-typescript": "npm install typescript@latest --save-dev && npm install typescript@latest --workspace=* --save-dev"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "turbo": "latest",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.2.3",
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.503.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  }
}
